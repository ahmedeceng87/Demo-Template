{
  "$schema": "http://json.schemastore.org/template",
  "author": "Ahmed",
  "classifications": [ "Console" ],
  "name": "Demo Template",
  "identity": "Templates.Demo.CSharp",
  "shortName": "Demo",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "Templates",
  "preferNameDirectory": true,
  "symbols": {
    "CopyrightName": {
      "type": "parameter",
      "defaultValue": "Mohd Ahmed",
      "replaces": "NamePlaceHolder",
        "isRequired": true
    },
    "Title": {
      "type": "parameter",
      "defaultValue": "Hello World from dotnet new",
      "replaces": "TitlePlaceHolder"
    }
  }
}
